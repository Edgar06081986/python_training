# print(solution([1,2,3,4])) 



def solution(array1, array2):
    list_asc_1 = sorted(array1)
    list_asc_2 = sorted(array2)
    result = [x for x in list_asc_1 if x not in list_asc_2]
    return result
    


print(solution([1, 2, 2, 2, 3, 4, 6, 7],[1, 3]))



# [1, 3, 4, 5, 6, 8, 10]